<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <title>@ViewBag.Title - My ASP.NET MVC Application</title>
        <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
        <meta name="viewport" content="width=device-width" />
        @Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")
        <iframe src="//www.facebook.com/plugins/like.php?href=http%3A%2F%2Fdamonshuter.com&amp;width=100&amp;layout=standard&amp;action=like&amp;show_faces=true&amp;share=true&amp;height=80" scrolling="no" frameborder="0" style="border:none; overflow:hidden; width:100px; height:80px;" allowtransparency="true"></iframe>
    </head>
    <body>
       
        
        <header>
            <div class="content-wrapper">
                <div class="float-left">
                    <img src="~/Images/hoerbiger_logo_02_140.gif" />
                    <p class="site-title">@Html.ActionLink(" ", "Index", "Home")</p>
                </div>
                <div class="float-right">
                    <section id="login">
                        @Html.Partial("_LoginPartial")
                    </section>
                    <nav>
                        <ul id="menu">
                            <li>@Html.ActionLink("Home", "Index", "Home")</li>
                            <li>@Html.ActionLink("About", "About", "Home")</li>
                            <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                            <li>@Html.ActionLink("FactoryViewer", "FactoryViewer", "Home")</li>
                            <li>@Html.ActionLink("Student", "Index", "Student")</li>
                            <li>@Html.ActionLink("Game", "SampleGame", "Game")</li>
                            <li>@Html.ActionLink("Coming Soon", "Addtowebsite", "Home")</li>
                            <li>
                                @Html.ActionLink("Blog", "Blog", "Home")
                                <ul>
                                    <li><a href="#">C#</a></li>
                                    <li><a href="#">Sql</a></li>
                                </ul>
                            <li>@Html.ActionLink("New Products", "Index", "Products")</li>
                            </li> 
                              

                        </ul>
                    </nav>
                </div>
            </div>
        </header>
        <div id="body">
            @RenderSection("featured", required: false)
            <section class="content-wrapper main-content clear-fix">
                @RenderBody()
            </section>
        </div>
        <footer>
            <div class="content-wrapper">
                <div class="float-left">
                    <p>&copy; @DateTime.Now.Year - Damon Shuter</p>
                </div>
            </div>
        </footer>

        @Scripts.Render("~/bundles/jquery")
        @RenderSection("scripts", required: false)
    </body>
</html>
